<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="setra.propulsar.com.sectrab">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_login"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoBar">
        <activity
            android:name=".presentationlayer.activities.SplashActivity"
            android:theme="@style/AppThemeNoBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentationlayer.activities.LoginActivity"
            android:theme="@style/AppThemeNoBar" />
        <activity
            android:name=".presentationlayer.activities.RegisterActivity"
            android:parentActivityName=".presentationlayer.activities.LoginActivity"
            android:theme="@style/AppThemeNoBar" />
        <activity
            android:name=".presentationlayer.activities.DummyActivity"
            android:theme="@style/AppThemeNoBar" />
        <activity
            android:name=".presentationlayer.activities.MainNavigationActivity"
            android:theme="@style/AppThemeNoBar"/>
    </application>

</manifest>